<form method="post" action="{{ path('course_delete', {'id': course.id}) }}" onsubmit="return confirm('Вы уверены, что хотите удать курс?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ course.id) }}">
    <button class="btn btn-primary mt-3">Удалить курс</button>
</form>
